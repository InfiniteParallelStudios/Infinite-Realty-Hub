        }
      ]
    },
    {
      "name": "Contacts",
      "short_name": "Contacts",
      "description": "Manage your contacts",
      "url": "/contacts",
      "icons": [
        {
          "src": "/icons/icon-96x96.svg",
          "sizes": "96x96"
        }
